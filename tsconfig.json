{
	"compilerOptions": {
		"target": "esnext",
		"lib": ["esnext"],
		"module": "esnext",
		"moduleResolution": "node",
		"types": [
			"@types/node",
			"@cloudflare/vitest-pool-workers",
			"vitest",
			"./worker-configuration.d.ts"
		],
		"noEmit": true,
		"isolatedModules": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"strict": true
	},
	"include": ["src", "tests"]
}
